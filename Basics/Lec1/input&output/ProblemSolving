/* 
Input Format
One line that contains 3 space-separated integers: a, b, and c.
Constraints
1 <= a,b,c <=1000
Output Format
Print the sum of the three numbers on a single line. 
*/


#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int x;
    int y;
    int z;
    cin >> x >> y >> z;
    cout << x + y + z;
    return 0;
}